@model SmartFoodRestaurantSystem.Models.Category

@{
    ViewData["Title"] = "Details";
}

<h1 class="h4 text-gray-900 mb-0">Category Details</h1>

<div >
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Status)
        </dt>
